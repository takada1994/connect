<%= form_tag :search_users, method: :get, class: "serch" do %>
    <%= text_field_tag "q", params[:q] %>
    <%= submit_tag "検索" %>
<% end %>

<% if @users.any? %>
    <% @users.each do |user| %>
    <div class = "user_index_item">
        <%= link_to user.name , user_path(user)%>
</div>
<% end%>
<% else%>
    <p>まだユーザーは登録されていません</p>
<% end%>